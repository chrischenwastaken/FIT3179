{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "layer": [
      
      {
        "data": {
          "url": "https://raw.githubusercontent.com/chrischenwastaken/FIT3179/refs/heads/main/suburb-2-vic.geojson",
          "format": {
            "type": "json",
            "property": "features"
          }
        },
        "mark": {
          "type": "geoshape",
          "fill": "lightgrey"
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/chrischenwastaken/FIT3179/refs/heads/main/Traffic_Volume1.geojson",
          "format": {
            "type": "json",
            "property": "features"
          }
        },
        "mark": {
          "type": "geoshape",
          "strokeWidth": 1
        },
        "encoding": {
          "stroke": {
            "field": "properties.ALLVEHS_AADT",
            "type": "quantitative",
            "scale": {
              "scheme": "reds",
              "domain": [0, 50, 80000]  
            },
            "legend": {
              "title": "Average Annual Daily Traffic"
            }
          },
          "tooltip": [
            {"field": "properties.HMGNS_LNK_DESC", "type": "nominal", "title": "Street"},
            {"field": "properties.ALLVEHS_AADT", "type": "quantitative", "title": "Avg. Annual Daily Traffic"},
            {"field": "properties.LGA_SHORT_NM", "type": "nominal", "title": "LGA"},
            {"field": "properties.RGN_LONG_NM", "type": "nominal", "title": "Region"},
            {"field": "properties.ROAD_NBR", "type": "nominal", "title": "Road Number"},
            {"field": "properties.DECLARED_ROAD_NM", "type": "nominal", "title": "Declared Road Name"},
            {"field": "properties.FLOW", "type": "nominal", "title": "Flow Direction"},
            {"field": "properties.YR", "type": "nominal", "title": "Year"}
          ]
        },
        "projection": {
          "type": "mercator"
        }
      }
    ],
    "width": 800,
    "height": 600,
    "title": "Traffic Volume Map of Victoria (2020)"
  }